view external {
    match-clients { any; };
    zone "wt1.ephec-ti.be" {
        type master;
        file "/etc/bind/db.wt1.ephec-ti.be";
    };
};

acl internals {
    54.37.65.117;
    54.37.65.136;
}

view internal {
    match-clients { internals; };
    recursion yes;
    zone "wt1.ephec-ti.be" IN  {
        type master;
        file "/etc/bind/db.internal.wt1.ephec-ti.be";

    };
}


